{
  "name": "synqforge",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered project management and agile workflow platform",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "validate-build": "tsx scripts/validate-build.ts",
    "test": "node --import tsx --test --test-reporter spec tests/**/*.test.ts",
    "test:ai-backlog-builder": "node --import tsx --test --test-reporter spec tests/integration/ai-backlog-builder.test.ts",
    "test:ai": "bash scripts/run-integration-tests.sh",
    "test:quick": "bash scripts/quick-test.sh",
    "test:unit": "node --import tsx --test --test-reporter spec tests/unit/**/*.test.ts",
    "test:integration": "node --import tsx --test --test-reporter spec tests/integration/**/*.test.ts",
    "smoke": "bash scripts/smoke.sh",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "setup": "npm run db:migrate",
    "embeddings:backfill": "tsx scripts/backfill-embeddings.ts",
    "embeddings:retry": "tsx scripts/retry-failed-embeddings.ts",
    "embeddings:validate": "tsx scripts/validate-semantic-search.ts",
    "test:embeddings": "node --import tsx --test --test-reporter spec tests/unit/embeddings.test.ts",
    "test:context-access": "node --import tsx --test --test-reporter spec tests/unit/context-access.test.ts",
    "test:context-api": "node --import tsx --test --test-reporter spec tests/integration/context-access-api.test.ts",
    "test:tier-access": "npm run test:context-access && npm run test:context-api",
    "validate:plans": "tsx scripts/validate-plans-production.ts",
    "test:plans": "vitest run __tests__/pricing-components.test.tsx",
    "test:plans:coverage": "vitest run --coverage __tests__/pricing-components.test.tsx",
    "deploy:api": "node scripts/deploy-api.js",
    "deploy:api:auto": "AUTO_COMMIT=true node scripts/deploy-api.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hubspot/api-client": "^13.4.0",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^0.5.7",
    "@react-email/render": "^1.4.0",
    "@sentry/nextjs": "^10.22.0",
    "@stripe/stripe-js": "^8.0.0",
    "@tanstack/react-query": "^5.90.5",
    "@types/bcrypt": "^6.0.0",
    "@types/diff": "^7.0.2",
    "@uploadthing/react": "^7.3.3",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.5",
    "ably": "^2.14.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "date-fns": "^4.1.0",
    "diff": "^8.0.2",
    "docx": "^9.5.1",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "mammoth": "^1.11.0",
    "mysql2": "^3.11.0",
    "nanoid": "^5.0.0",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "openai": "^4.82.0",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "recharts": "^3.2.1",
    "resend": "^6.1.2",
    "sonner": "^2.0.7",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uploadthing": "^7.7.4",
    "xlsx": "^0.18.5",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.49.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.0.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/pdfkit": "^0.17.3",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "esbuild": "^0.25.10",
    "eslint": "9.37.0",
    "eslint-config-next": "15.5.4",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vitest": "^4.0.8"
  },
  "overrides": {
    "esbuild": "$esbuild"
  }
}
